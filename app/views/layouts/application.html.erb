<!DOCTYPE html>
<html>
<head>
  <title>FitnessPlanner</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
</head>

<body>
<% if current_user %>
  <a href="/user">Home</a>
  <a href="/user/show">My Profile</a>
  <a href="/logout">Log out</a>
  <strong> Currently signed in as <%= current_user.name %></strong>
<% else %>
  <a href="/user">Home</a>
  <a href="/sessions/new">Log in</a>
  <a href="/user/new">Register</a>
<% end %><br>
<%= yield %>
</body>
</html>
