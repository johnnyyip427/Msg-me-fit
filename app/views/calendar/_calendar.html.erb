
<div >
  <br><br><%= month_calendar events: @user_appointments do |date, appointments| %>
  <%= date.strftime('%d') %>

  <% appointments.each do |appointment| %>
    <div class="calendar">
      <a href="/user/<%=@user_id%>/calendar/<%=appointment.id%>"><strong><%= appointment.title %></strong><%= appointment.start_time.strftime(" %I:%M%p") %></a>
      <% end %>
    </div>
  <% end %>
</div>

